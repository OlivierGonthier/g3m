<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.roly.GiveMeMyMoney2"
    android:versionCode="14"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.roly.GiveMeMyMoney2.G3MApplication"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/Theme.GDAB" >
        <activity
            android:name=".G3MActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />
        <activity android:name=".ResumeActivity" />
        <activity android:name=".AddActivity" />
        <activity
            android:name="MoneyDialog"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity android:name="com.roly.GiveMeMyMoney2.PretsListActivity" >
        </activity>
    </application>

</manifest>